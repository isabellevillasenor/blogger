<h1 style="color:SlateBlue;">All Articles</h1>

<ul id="articles" style="color:Violet;" >
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article), class: 'css_class', style: 'color:Violet'%>
    </li>
  <% end %>
  <h4><%= link_to 'Create New Article', new_article_path, class: 'css_class', style: 'color:#65c485'%></h4>
</ul>
